<PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory app:title="基础设置">

        <EditTextPreference
            app:icon="@drawable/add"
            app:title="添加检测关键字"
            app:summary="有时候写了关闭会自动关闭小程序，但是不写关闭无法自动跳广告0.0"
            app:dialogTitle="多个关键字请以空格分隔"
            app:key="keyword"/>

        <Preference
            app:icon="@drawable/white_apps"
            app:key="whitelist"
            app:title="应用白名单" />

        <ListPreference
            app:icon="@drawable/speed"
            app:dialogTitle="勾选想要的倍速"
            app:entries="@array/speed_entries"
            app:entryValues="@array/speed_entry_values"
            app:defaultValue="100"
            app:key="speed"
            app:title="调节倍速" />
    </PreferenceCategory>


</PreferenceScreen>